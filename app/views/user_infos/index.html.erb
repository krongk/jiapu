<h1>Listing user_infos</h1>

<table>
  <tr>
    <th>User</th>
    <th>Xing</th>
    <th>Ming</th>
    <th>En name</th>
    <th>Nice name</th>
    <th>Gender</th>
    <th>Birth</th>
    <th>Birth place</th>
    <th>Living place</th>
    <th>Picture url</th>
    <th>Email</th>
    <th>Qq</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_infos.each do |user_info| %>
  <tr>
    <td><%= user_info.user %></td>
    <td><%= user_info.xing %></td>
    <td><%= user_info.ming %></td>
    <td><%= user_info.en_name %></td>
    <td><%= user_info.nice_name %></td>
    <td><%= user_info.gender %></td>
    <td><%= user_info.birth %></td>
    <td><%= user_info.birth_place %></td>
    <td><%= user_info.living_place %></td>
    <td><%= user_info.picture_url %></td>
    <td><%= user_info.email %></td>
    <td><%= user_info.qq %></td>
    <td><%= user_info.phone %></td>
    <td><%= link_to 'Show', user_info %></td>
    <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
    <td><%= link_to 'Destroy', user_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

