<h2>搜索结果</h2>
<table class="table table-striped table-hover">
<% if @search_items.empty? %>
  <tr><td><div class="well">没有搜到任何结果!</div></td></tr>
<% end %>
<% @search_items.each do |item| %>
  <tr>
    <td width="100px"><%= item[:created_at].strftime("%Y-%m-%d") %></td>
    <td><%=link_to item[:title], item[:url] %></td>
  </tr>
<% end %>
</table>