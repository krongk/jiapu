<%= simple_form_for(@upload) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :resource_id %>
    <%= f.input :upload_file_name %>
    <%= f.input :upload_content_type %>
    <%= f.input :upload_file_size %>
    <%= f.input :upload_updated_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
